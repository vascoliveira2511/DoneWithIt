{
  "expo": {
    "name": "donewithit",
    "slug": "donewithit",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,

    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e63c4b"
    },

    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./app/assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#e63c4b"
      }
    },

    "android": {
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./app/assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#e63c4b"
      }
    },

    "web": {
      "favicon": "./app/assets/favicon.png"
    },

    "extra": {
      "eas": {
        "projectId": "a74e8d13-cde4-4f29-956b-1d80b8c3268e"
      }
    },

    "owner": "plateau",
    "runtimeVersion": {
      "policy": "appVersion"
    },

    "updates": {
      "url": "https://u.expo.dev/a74e8d13-cde4-4f29-956b-1d80b8c3268e"
    },

    "plugins": ["expo-secure-store"]
  }
}
