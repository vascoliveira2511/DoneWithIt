{
  "expo": {
    "name": "donewithit",
    "slug": "donewithit",
    "version": "1.0.0",
    "icon": "./app/assets/icon.png",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e63c4b"
